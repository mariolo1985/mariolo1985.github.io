{
  "name": "react-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Minimalistic ES6 React boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf wwwroot/dist",
    "build": "cross-env npm run clean && webpack --config webpack.development.config.js --progress --profile --colors",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test",
    "build:scss": "sass ./wwwroot/app/assets/scss/main.scss ./wwwroot/dist/styles/styles.css",
    "build:component": "babel ./wwwroot/app/components --out-dir ./wwwroot/build",
    "build:injector": "webpack --config webpack.mlo.config.js -p",
    "build:devinjector": "webpack --config webpack.mlo.config.js",
    "build:prod": "npm run build:scss && npm run build:component && npm run build:injector"
  },
  "author": "Kliment Petrov <kleopetroff@gmail.com>",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/shim.js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "dependencies": {
    "angular": "1.6.6",
    "babel-polyfill": "6.26.0",
    "bootstrap": "3.3.7",
    "bootstrap-sass": "3.3.7",
    "cross-env": "5.1.0",
    "font-awesome": "4.7.0",
    "foundation-sites": "6.4.4-rc1",
    "htmltojsx": "0.3.0",
    "import-glob-loader": "1.1.0",
    "jquery": "3.2.1",
    "jquery-ajax-unobtrusive": "3.2.4",
    "jquery-validation": "1.17.0",
    "material-design-icons-iconfont": "3.0.3",
    "react": "16.0.0",
    "react-cookie": "^2.1.2",
    "react-dom": "16.0.0",
    "react-hot-loader": "3.1.1",
    "react-router-dom": "4.2.0",
    "universal-cookie": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es2015-template-literals": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-ie": "6.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.1.1",
    "css-loader": "0.28.7",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.2.0",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.4",
    "html-webpack-plugin": "2.30.0",
    "husky": "0.15.0-beta.2",
    "jest": "21.2.0",
    "node-sass": "4.5.3",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.0.0",
    "resolve-url-loader": "2.1.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.1",
    "webpack": "3.8.0",
    "webpack-dev-server": "2.9.2"
  }
}
